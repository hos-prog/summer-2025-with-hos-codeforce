# HOsamaAdem
t = int(input())

def is_lucky(n):
    for digit in str(n):
        if digit != '4' and digit != '7':
            return False
    return True

# check all lucky numbers from 1 to 1000
found = False
for i in range(1, 1001):
    if is_lucky(i) and t % i == 0:
        found = True
        break

if found:
    print("YES")
else:
    print("NO")
