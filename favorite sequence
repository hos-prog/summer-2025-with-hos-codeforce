#hos
t=int(input())
for _ in range(t):
    n= int(input())
    a = list(map(int, input().split()))
    left = 0
    right = n - 1
    list1=[]
    while len(list1)<len(a):
        list1.append(a[left])
        left+=1
        if len(list1)<len(a): 
            list1.append(a[right])
            right-=1
    print(*list1)    
        


