#HosamaAdem
t=int(input())


for _ in range(t):
    m=list(map(int,input().split()))
    v=sorted(m)
    wi_1=m[0]
    wi_2=m[0]

    if m[0] >m[1]:
        wi_1=m[0]
    elif m[0]<m[1]:
        wi_1=m[1]
    if m[2]>m[3]:
        wi_2=m[2]
    elif m[2]<m[3]:
        wi_2=m[3]
    if (wi_1==v[2] or wi_1==v[3]) and (wi_2==v[3] or wi_2==v[2]):
        print("YES")
    else:
        print("NO")
