#hos
t = int(input())
for _ in range(t):
    s = input()
    hour = int(s[:2])
    minute = s[2:]  

    if hour == 0:
        print(f"12{minute} AM")
    elif hour == 12:
        print(f"12{minute} PM")
    elif hour > 12:
        print(f"{hour - 12:02}{minute} PM")  
    else:
        print(f"{hour:02}{minute} AM")