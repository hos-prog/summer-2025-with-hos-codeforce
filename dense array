#Hosama Adem
t = int(input())

for _ in range(t):
    n = int(input())
    a = list(map(int, input().split()))

    count = 0
    for i in range(n - 1):
        x, y = a[i], a[i + 1]
        sm= min(x, y)
        la = max(x, y)
        while la > 2 * sm:
            sm *= 2
            count += 1
    print(count)
